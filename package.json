{
  "version": "0.0.4",
  "name": "@torian12321/prettier-config",
  "description": "Prettier config",
  "author": "torian12321",
  "main": "index.js",
  "types": "index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./src/index.js",
      "types": "./src/index.d.ts"
    }
  },
  "scripts": {
    "validate": "node scripts/validate-config.js",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "npm run validate",
    "prepublishOnly": "npm run validate && npm run format:check"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/torian12321/prettier-config.git"
  },
  "devDependencies": {
    "prettier": "^3.6.2"
  }
}
